{
  "name": "use-wagmi",
  "description": "Vue Composition for Ethereum based on wagmi",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unicape/use-wagmi.git",
    "directory": "packages/vue"
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "pnpm run clean && pnpm run build:esm+types",
    "build:esm+types": "tsc --project tsconfig.build.json --module es2020 --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./actions": {
      "types": "./dist/types/actions.d.ts",
      "default": "./dist/esm/actions.js"
    },
    "./chains": {
      "types": "./dist/types/chains.d.ts",
      "default": "./dist/esm/chains.js"
    },
    "./connectors": {
      "types": "./dist/types/connectors.d.ts",
      "default": "./dist/esm/connectors.js"
    },
    "./query": {
      "types": "./dist/types/query.d.ts",
      "default": "./dist/esm/query.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "actions": ["./dist/types/actions.d.ts"],
      "chains": ["./dist/types/chains.d.ts"],
      "connectors": ["./dist/types/connectors.d.ts"],
      "query": ["./dist/types/query.d.ts"]
    }
  },
  "peerDependencies": {
    "@tanstack/vue-query": "5.0.0-beta.18",
    "typescript": ">=5",
    "viem": "^1",
    "vue": ">=3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@wagmi/chains": "^1.7.0",
    "@wagmi/connectors": "^2.7.0",
    "@wagmi/core": "^1.3.9"
  },
  "devDependencies": {
    "@tanstack/vue-query": "^4.29.11",
    "vue": "^3.3.4"
  },
  "keywords": ["wagmi", "viem", "vue", "hooks", "eth", "ethereum", "web3"],
  "contributors": ["Unic Ape <94770222@qq.com>"],
  "homepage": "https://github.com/unicape/use-wagmi"
}
